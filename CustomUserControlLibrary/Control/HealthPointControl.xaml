<UserControl x:Class="CustomUserControlLibrary.Control.HealthPointControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:SIn="clr-namespace:CustomUserControlLibrary.Converter"
             xmlns:tran="clr-namespace:CustomUserControlLibrary.Converter"
             mc:Ignorable="d" >
    <UserControl.Resources>
        <SIn:HealthShowConvert x:Key="HealthShowConverter"></SIn:HealthShowConvert>
    </UserControl.Resources>
    <Grid Height="15" Width="300">
        <Border HorizontalAlignment="Left" Height="15" Width="151"  Background="White" CornerRadius="7.5 0 0 7.5"  BorderThickness="0"/>
        <ProgressBar Height="15" Width="300" Value="{Binding testText,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"  Style="{StaticResource  ProgressBarStyle}"  >
            
        </ProgressBar>
      
    </Grid>
  
</UserControl>
